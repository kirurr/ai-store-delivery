{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "Orders"
    },
    "price": {
      "type": "float",
      "required": true
    },
    "orderStatus": {
      "type": "enumeration",
      "required": true,
      "default": "Pending",
      "enum": [
        "Done",
        "Pending",
        "Canceled"
      ]
    },
    "commentary": {
      "type": "text"
    },
    "address": {
      "type": "text",
      "required": true
    }
  }
}
